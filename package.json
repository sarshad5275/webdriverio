{
  "name": "webdriverio-tests",
  "version": "1.0.0",
  "description": "Automation Framework for WebdriverIO",
  "main": "index.js",
  "scripts": {
    "bamboo": "wdio run wdio.mochaconf.js --spec ./test/specs/bamboo.js",
    "nexport": "wdio run wdio.mochaconf.js --spec ./test/specs/nexport.js",
    "nexapi": "wdio run wdio.mochaconf.js --spec ./test/specs/nexAPI.js",
    "examples": "wdio run wdio.mochaconf.js --spec ./test/specs/dbtest.js",
    "wdio_mocha": "wdio run wdio.mochaconf.js",
    "encrypt": "wdio run wdio.mochaconf.js --spec ./test/specs/encryptpassword.js",
    "wdio_cucumber": "wdio run wdio.cucumberconf.js",
    "cucumber": "wdio run ./wdio.cucumberconf.js ",
    "cucumbertag": "wdio run ./wdio.cucumberconf.js --cucumberOpts.tagExpression",
    "apitest": "wdio run ./wdio.cucumberconf.js --cucumberOpts.tagExpression='@DBTest'",
    "clean": "rimraf allure-results && rimraf allure-report && rimraf test-screenshots && rimraf wdio-logs && rimraf features/cucumber-reports/",
    "allure": "allure generate --clean allure-results && allure open",
    "open": "allure open",
    "jenkins": "java -jar jenkins.war"
  },
  "keywords": [],
  "author": "Nexient",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/node": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/register": "^7.16.7",
    "@wdio/allure-reporter": "^7.16.13",
    "@wdio/cli": "^7.16.13",
    "@wdio/cucumber-framework": "^7.16.14",
    "@wdio/local-runner": "^7.16.14",
    "@wdio/mocha-framework": "^7.16.13",
    "@wdio/spec-reporter": "^7.16.14",
    "chai": "^4.3.4",
    "chromedriver": "^99.0",
    "cryptr": "^6.0.2",
    "cucumber-html-reporter": "^5.5.0",
    "fs-extra": "^10.0.1",
    "moment": "^2.29.1",
    "multiple-cucumber-html-reporter": "^1.19.0",
    "rimraf": "^3.0.2",
    "superagent": "^7.0.2",
    "supertest": "^6.1.6",
    "wdio-allure-reporter": "^0.8.3",
    "wdio-chromedriver-service": "^7.2.6",
    "wdio-cucumberjs-json-reporter": "^4.4.0",
    "wdio-video-reporter": "^3.2.0",
    "wdio-wait-for": "^2.2.1"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^14.3.2",
    "mysql": "^2.18.1",
    "prettier": "^2.5.1"
  }
}
